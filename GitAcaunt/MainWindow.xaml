<Window x:Class="GitAcaunt.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GitAcaunt"
        mc:Ignorable="d"
        Title="ChangUser" Height="150" Width="300">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Button  x:Name="btn6N2P"
            Height="30" Margin="5" Click="Button_Click_6N2P">
            6N2p
        </Button>
        <Button x:Name="btnSergey"
            Grid.Column="1" Height="30" Margin="5" Click="Button_Click_Sergey">
            sergey
        </Button>
        <TextBlock x:Name="tb_Text"
                   Grid.Row="1"
                   Grid.ColumnSpan="2"
                   FontSize="20"
                   Margin="5"
                   VerticalAlignment="Center"
                   HorizontalAlignment="Center"/>
    </Grid>
</Window>
